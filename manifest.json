{
  "manifest_version": 2,
  "version": "0.1",
  "name": "check flow",
  "short_name": "terumit",
  "description": "フロント制作フローチェックリスト",
  "browser_action": {
    "default_title": "terumit",
    "default_popup": "popup.html"
  },
  "content_scripts":[
    {
      "matches":["https://*/*","http://*/*"],
      "js":[
        "script.js",
        "jquery.min.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "*://*/*/",
    "webRequest",
    "tabs"
  ]
}
